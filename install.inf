[info]
title=Formatter for Pascal/C++ using Embarcadero Formatter
desc=Formats Pascal and C++ code using Embarcadero's Formatter.exe
type=cudatext-plugin
subdir=cuda_fmt_embarcadero
homepage=https://github.com/CudaText-addons/cuda_fmt_embarcadero

[fmt1]
caption=Embarcadero Format
method=do_format_pas
